InteractionsWithMissingCRMAccess :=
COUNTROWS (
    FILTER (
        VALUES ( comm_emp[comm_id] ),
        CALCULATE (
            COUNTROWS ( comm_emp ),
            FILTER (
                comm_emp,
                ISBLANK (
                    LOOKUPVALUE (
                        access_list[resp_id],
                        access_list[per_id], comm_emp[emp_id]
                    )
                )
            )
        ) > 0
    )
)
