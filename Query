Interactions_With_All_InactiveEmp :=
CALCULATETABLE (
    VALUES ( interaction[comm_id] ),
    NOT ISEMPTY (
        SELECTCOLUMNS (
            FILTER (
                comm_emp,
                RELATED ( contact[x_status] ) <> "Active"
                    || ISBLANK ( RELATED ( contact[x_status] ) )
            ),
            "comm_id", comm_emp[comm_id]
        )
    )
)
